{            
    "request": {
        "method": "POST",
        "urlPattern": "/v2/dropshipment",
        "headers": {
            "Authorization": {
                "doesNotMatch": "Bearer ($|&.*)"
            },
            "Content-Type": {
                "matches": "application/x-www-form-urlencoded"
            }
        },
        "bodyPatterns": [
            {
                "matches": "(.*&|^)action=delete($|&.*)"
            },
            {
                "matches": "(.*&|^)client_id=test_client_id($|&.*)"
            },
            {
                "matches": "(.*&|^)nonce=test_nonce($|&.*)"
            },
            {
                "matches": "(.*&|^)signature=test_signature_sample($|&.*)"
            },
            {
                "matches": "(.*&|^)order_ids=[(.)order_id_1(.),(.)order_id_2(.)]($|&.*)"
            },
            {
                "matches": "(.*&|^)customer_ref_ids=[(.)customer_ref_id_1(.),(.)customer_ref_id_2(.)]($|&.*)"
            }
        ]
    },
    "response": {
        "status": 200,
        "bodyFileName": "Dropshipment/Dropshipmentv2-Getorderstatus.json",
        "headers": {
            "Content-Type": "application/json"
        }
    }
}